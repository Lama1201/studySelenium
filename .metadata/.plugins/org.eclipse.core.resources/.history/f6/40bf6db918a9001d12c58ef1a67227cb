package reviewJava;

public class methodDemo {
	
	// Tạo method return string
	public String getData() {
		return "Lama in dahouse";
		}
	
	// Tạo method ko return data
	public void printdemo() {
		System.out.println("This is demo of print");
	}
	
	
	public static void printdemo2() {
		System.out.println("This is demo of print");
	}

}
