package reviewJava;

import java.util.ArrayList;

public class reviewJava2 {
	public static void main(String[] args) {
		// Sử dụng class Arraylist để tạo mảng động
		ArrayList<String> name = new ArrayList<String>();
		name.add("Bao");
		name.add("Toan");
		
		for(String n: name ) {
			System.out.println(n);
		}	
	}
}
